# HLS Stress Test Configuration
# Copy this file to .env and customize as needed

# Stream page URL (required)
STREAM_URL=https://worldofwow.dev/assets/streams/example.html

# Stream bitrate in Kbps (used to calculate max browsers)
STREAM_BITRATE_KBPS=2800

# Optional overrides
# MAX_BROWSERS=10
# HEADLESS=true
# LAUNCH_DELAY_MS=1000
# BANDWIDTH_SAFETY_MARGIN=0.8
